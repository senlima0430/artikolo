{
  "name": "artikolo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 78,
    "useTabs": false,
    "endOfLine": "lf",
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@reach/router": "^1.3.3",
    "@testing-library/dom": "^7.10.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^12.0.0",
    "@types/prop-types": "^15.7.3",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^6.0.0",
    "@types/recoil": "^0.0.1",
    "@types/styled-components": "^5.1.2",
    "@types/styled-system": "^5.1.10",
    "immer": "^6.0.9",
    "lodash": "^4.17.19",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-is": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-scripts": "3.4.3",
    "recoil": "^0.0.10",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "typescript": "~3.7.2",
    "use-immer": "^0.4.0"
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6"
  }
}
